<div class="content">
  <div class="container">
    <div class="sixteen columns">
      <header>
        <h1>Viewing Run</h1>
      </header>

      <a style="text-decoration: none" href="#" onclick="showHide('show_inputs')" id="show_show_inputs">[+] View Related Inputs</a>
      <br/>
      <div id="show_inputs" class="hidden notvisible">
        <%= render 'show_inputs' %>
      </div>

      <br>
      <hr>

      <dl class="dl-horizontal">
        <dt>Model</dt>
        <dd><%= link_to @run.model, @run.model %></dd>
        <dt>Site</dt>
        <dd><%= link_to @run.site, @run.site %></dd>
        <dt>Start Time:</dt>
        <dd><%= @run.start_time %></dd>
        <dt>Finish Time:</dt>
        <dd><%= @run.finish_time %></dd>
        <dt>Started at:</dt>
        <dd><%= @run.started_at %></dd>
        <dt>Finshed at:</dt>
        <dd><%= @run.finished_at %></dd>
        <dt>Outdir:</dt>
        <dd><%= @run.outdir %></dd>
        <dt>Outprefix:</dt>
        <dd><%= @run.outprefix %></dd>
        <dt>Setting:</dt>
        <dd><%= @run.setting %></dd>
        <dt>Parameter List:</dt>
        <dd><%= @run.parameter_list %></dd>
      </dl>

      <div class="form-actions">
        <div class="button-group">
          <%= link_to( runs_path ) do %>
            <button class="button"><i class="icon-arrow-left"></i> All Records</button>
          <% end %>
        </div>
      </div>
    </div>
  </div>
</div>
